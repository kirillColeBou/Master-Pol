<Page x:Class="Master_Pol.Pages.Partner.Main"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Master_Pol.Pages.Partner"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Main">

    <Grid Background="White">
        <Image Source="/Images/logo.png" Width="50" Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,0,0"/>
        <Label Content="Поставщики" FontFamily="Segoe UI" Margin="90,25,0,0" FontSize="20" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Border BorderBrush="Black" BorderThickness="1" Margin="20,90,20,20">
            <ScrollViewer HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden">
                <StackPanel x:Name="parent"/>
            </ScrollViewer>
        </Border>
    </Grid>
</Page>
